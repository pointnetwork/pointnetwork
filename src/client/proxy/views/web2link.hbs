<html style="height: 100%;">
<head>
    <style>
        /* unvisited link */
        a:link {
            color: #90e8dc;
        }

        /* visited link */
        a:visited {
            color: #90e8dc;
        }

        /* mouse over link */
        a:hover {
            color: #e8e8e8;
        }

        /* selected link */
        a:active {
            color: #e8e8e8;
        }
    </style>
</head>

<body style='overflow: hidden; background: linear-gradient(45deg, #2f095e, #2d0000); background-size: cover; display: flex; justify-content: center; align-items: center; height: 100%;'>
<div style='text-align:center; width: 100%;'>
    <img style="filter: invert(1);width: 84px;margin-bottom: 1em;"
         src="data:image/png;base64,"/>
    <h1 style='font-size: 30px; color: #ccc; margin: 0; padding: 0;'>
        This seems to be a web2 link. Do you want to open it?<br/>
    </h1>
    <div style='font-size: 20px;padding: 0 20%; color: #e8e8e8; margin-top: 10px; font-family: sans-serif;'>
        Click the link below to open it in your default web2 browser:<br/><br/><br/>
        <a href="#" onclick="javascript:openWeb2Url('{{url}}')">{{ url }}</a>
    </div>
</div>
<script>
    async function openWeb2Url(url) {
        fetch('/v1/api/web2/open', {
            method: 'POST',
            headers: {
                'Accept': 'application/json',
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({urlToOpen: url, _csrf: '{{csrfToken}}', host: '{{host}}'})
        });//send the request with csrf token.
    }
</script>
</body>

</html>
