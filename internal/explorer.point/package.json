{
  "name": "explorer.point",
  "version": "0.0.1",
  "author": "Point Network",
  "description": "Template example zapp - use as a scaffold to create more Zapps!",
  "dependencies": {
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/bignumber": "^5.6.2",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.2",
    "@openzeppelin/contracts": "^4.4.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.3.0-alpha1",
    "dayjs": "^1.11.3",
    "json-source-map": "^0.6.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.7",
    "qrcode.react": "^3.0.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.0",
    "react-bootstrap-icons": "^1.5.0",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "sweetalert2": "^11.4.8",
    "timeago.js": "^4.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "clear": "rm -rf ./public \".cache\" \".parcel-cache\" && mkdir public",
    "start": "react-scripts start",
    "watch:docker": "./watch.docker.js",
    "prebuild": "npm ci",
    "build": "npm run clear && cp src/*.zhtml \"./public/.\" && cp src/*.css \"./public/.\" && cp -r src/assets \"./public/.\" && cp src/favicon.ico \"./public/.\" && parcel build \"src/index.js\" --dist-dir \"./public\" --no-source-maps",
    "watch": "npm run clear && cp src/*.zhtml \"./public/.\" && cp src/*.css \"./public/.\" && cp -r src/assets \"./public/.\" && cp src/favicon.ico \"./public/.\" && parcel watch \"src/index.js\" --dist-dir \"./public\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js --ext .jsx src/",
    "fix": "eslint --ext .js --ext .jsx --fix src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^14.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "lint-staged": "^13.0.3",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.6.2"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix"
  }
}
